{
  "extends": ["plugin:prettier/recommended"],
  "plugins": ["prettier", "import"],
  "rules": {
    "prettier/prettier": "error",
    "max-len": [
      "error", { "code": 140, "ignoreTemplateLiterals": false }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
}